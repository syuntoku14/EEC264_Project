author = {Dehghannasiri, Roozbeh and Esfahani, Mohammad Shahrokh and Qian, Xiaoning and Dougherty, Edward R.},
doi = {10.1109/TSP.2017.2788419},
file = {:home/syuntoku14/OneDrive/Papers/Dehghannasiri et al. - 2018 - Optimal Bayesian Kalman Filtering with Prior Update(3).pdf:pdf},
issn = {1053587X},
