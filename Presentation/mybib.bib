keywords = {Bayesian robustness,Kalman filter,factor graphs,posterior distribution,uncertain noise statistics},
number = {8},
pages = {1982--1996},
publisher = {IEEE},
title = {{Optimal Bayesian Kalman Filtering with Prior Update}},
volume = {66},
year = {2018}
